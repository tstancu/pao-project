<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="pao.view.DoctorsController">
    <Label text="Doctor List" />

    <TableView fx:id="doctorTableView">
        <columns>
            <TableColumn text="ID" fx:id="idColumn" />
            <TableColumn text="Name" fx:id="nameColumn" />
            <TableColumn text="Specialty" fx:id="specialtyColumn" />
        </columns>
    </TableView>

    <Button fx:id="addButton" text="Add Doctor" onAction="#openDoctorForm" />
</VBox>






